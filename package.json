{
  "name": "autoqamate",
  "version": "1.0.0",
  "description": "AutoQAMate Desktop Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack serve --config webpack.config.js\" \"wait-on http://localhost:8080 && electron .\"",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode production",
    "package": "npm run build && electron-builder --dir",
    "make": "npm run build && electron-builder build --win --publish always"
  },
  "build": {
    "appId": "com.dawnscapelab.autoqamate",
    "productName": "AutoQAMate",
    "files": [
      "dist/**/*",
      "main.js",
      "package.json"
    ],
    "directories": {
      "output": "release",
      "buildResources": "assets"
    },
    "extraMetadata": {
      "main": "main.js"
    },
    "win": {
      "icon": "src/assets/icons/icon.ico"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.25.6",
    "@headlessui/react": "^2.1.3",
    "@heroicons/react": "^2.1.5",
    "@tailwindcss/forms": "^0.5.8",
    "crypto-browserify": "^3.12.0",
    "electron-squirrel-startup": "^1.0.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "puppeteer": "^23.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.41",
    "postcss-loader": "^8.1.1",
    "react-scripts": "^5.0.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.10",
    "wait-on": "^5.3.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
